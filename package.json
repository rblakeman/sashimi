{
    "name": "sashimi",
    "version": "0.0.1",
    "description": "",
    "author": "Ryan Blakeman",
    "keywords": [],
    "main": "src/index.js",
    "private": "true",
    "workspaces": [
        "apps/*",
        "libs/components/web/*",
        "libs/components/react/*"
    ],
    "scripts": {
        "setup": "yarn install && yarn setup:docs",
        "verify": "yarn lint && yarn typecheck",
        "lint": "eslint '**/*.{ts,tsx,js,jsx}'",
        "typecheck": "tsc --noEmit",
        "//": "Docusaurus",
        "setup:docs": "cd apps/sashimi-docs && yarn install",
        "start:docs": "cd apps/sashimi-docs && yarn start",
        "build:docs": "cd apps/sashimi-docs && yarn build",
        "serve:docs": "cd apps/sashimi-docs && yarn serve"
    },
    "dependencies": {
        "@types/node": "^18.7.23",
        "lit": "^2.3.1",
        "tslib": "^2.4.0",
        "typescript": "^4.8.4"
    },
    "devDependencies": {
        "@babel/preset-react": "^7.18.6",
        "@typescript-eslint/parser": "^5.28.0",
        "lerna": "^6.0.1"
    },
    "browserslist": [
        ">0.25%",
        "not ie 11",
        "not op_mini all"
    ]
}
